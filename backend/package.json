{
  "name": "patch-api",
  "scripts": {
    "build": "tsc -b",
    "serve": "yarn run build && firebase emulators:start --only functions",
    "shell": "yarn run build && firebase functions:shell",
    "start": "yarn run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "dev": "yarn run build && PATCH_LOCAL_ENV=dev node lib/backend/src/index.js",
    "staging": "yarn run build && PATCH_LOCAL_ENV=staging node lib/backend/src/index.js",
    "prod": "yarn run build && PATCH_LOCAL_ENV=prod node lib/backend/src/index.js",
    "migration:create": "mongo-migrate create",
    "migration:status": "node lib/backend/src/migrations/index.js status",
    "migration:up": "node lib/backend/src/migrations/index.js up",
    "migration:down": "node lib/backend/src/migrations/index.js down",
    "migration:down:latest": "node lib/backend/src/migrations/index.js down -l",
    "migration:status:staging": "_ENVIRONMENT=staging node lib/backend/src/migrations/index.js status",
    "migration:up:staging": "_ENVIRONMENT=staging node lib/backend/src/migrations/index.js up",
    "migration:down:staging": "_ENVIRONMENT=staging node lib/backend/src/migrations/index.js down",
    "migration:down:latest:staging": "_ENVIRONMENT=staging node lib/backend/src/migrations/index.js down -l",
    "migration:status:prod": "_ENVIRONMENT=prod node lib/backend/src/migrations/index.js status",
    "migration:up:prod": "_ENVIRONMENT=prod node lib/backend/src/migrations/index.js up",
    "migration:down:prod": "_ENVIRONMENT=prod node lib/backend/src/migrations/index.js down",
    "migration:down:latest:prod": "_ENVIRONMENT=prod node lib/backend/src/migrations/index.js down -l",
    "oclif": "oclif generate test_new_cli"
  },
  "engines": {
    "node": "12"
  },
  "main": "lib/backend/src/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^2.18.4",
    "@mapbox/timespace": "^2.0.4",
    "@oclif/command": "^1.8.22",
    "@oclif/plugin-autocomplete": "^2.1.8",
    "@oclif/plugin-commands": "^2.2.13",
    "@oclif/plugin-help": "^5.2.9",
    "@oclif/plugin-not-found": "^2.3.23",
    "@socket.io/redis-adapter": "^7.1.0",
    "@tsed/agenda": "^6.133.1",
    "@tsed/ajv": "^6.133.1",
    "@tsed/common": "^6.133.1",
    "@tsed/core": "^6.133.1",
    "@tsed/di": "^6.133.1",
    "@tsed/exceptions": "^6.133.1",
    "@tsed/json-mapper": "^6.133.1",
    "@tsed/mongoose": "^6.133.1",
    "@tsed/passport": "^6.133.1",
    "@tsed/platform-express": "^6.133.1",
    "@tsed/schema": "^6.133.1",
    "@tsed/socketio": "^6.133.1",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.3.3",
    "agenda": "^4.2.1",
    "ajv": "^8.6.2",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^4.5.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "expo-server-sdk": "^3.6.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "form-data": "^4.0.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "mailgun.js": "^8.0.1",
    "method-override": "^3.0.0",
    "module-alias": "^2.2.2",
    "mongo-migrate-ts": "^1.1.1",
    "mongoose": "6.2.6",
    "oclif": "^3.7.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "randomcolor": "^0.6.2",
    "redis": "^4.0.1",
    "socket.io": "^4.4.0",
    "twilio": "^3.71.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express-session": "^1.17.4",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mongodb": "^4.0.7",
    "@types/passport": "^1.0.7",
    "@types/passport-local": "^1.0.34",
    "@types/uuid": "^8.3.1",
    "firebase-functions-test": "^0.2.0",
    "typescript": "^4.0.2"
  },
  "private": true,
  "bin": "./bin/run",
  "oclif": {
    "commands": "./lib/backend/src/cli/commands",
    "bin": "backend",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-commands"
    ]
  },
  "_moduleAliases": {
    "common": "./lib/common",
    "infra/src": "./infra/lib/src"
  },
  "workspaces": [
    "infra"
  ],
  "packageManager": "yarn@3.1.1"
}
