{
  "cli": {
    "version": "^3.7.2"
  },
  "build": {
    "prod": {
      "channel": "prod",
      "env": {
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "_ENVIRONMENT": "prod"
      },
      "expoCli": "6.2.1"
    },
    "staging": {
      "extends": "prod",
      "distribution": "internal",
      "channel": "staging",
      "env": {
        "_ENVIRONMENT": "staging"
      }
    },
    "staging-simulator": {
      "extends": "staging",
      "ios": {
        "simulator": true
      }
    },
    "dev": {
      "extends": "staging",
      "developmentClient": "true",
      "distribution": "internal",
      "channel": "dev",
      "env": {
        "_ENVIRONMENT": "dev"
      }
    }
  }
}