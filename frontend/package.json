{
  "scripts": {
    "dev": "_DEV_ENVIRONMENT=dev expo start --dev-client",
    "dev:staging": "_DEV_ENVIRONMENT=staging expo start --dev-client",
    "dev:prod": "_DEV_ENVIRONMENT=prod expo start --dev-client",
    "build:prod": "_ENVIRONMENT=prod eas build --profile prod",
    "build:staging": "_ENVIRONMENT=staging eas build --profile staging",
    "build:staging-simulator": "_ENVIRONMENT=staging eas build --profile staging-simulator",
    "build:dev": "eas build --profile dev",
    "build:prod:ios": "_ENVIRONMENT=prod eas build --profile prod --platform ios",
    "build:staging:ios": "_ENVIRONMENT=staging eas build --profile staging --platform ios",
    "build:dev:ios": "eas build --profile dev --platform ios",
    "build:prod:android": "_ENVIRONMENT=prod eas build --profile prod --platform android",
    "build:staging:android": "_ENVIRONMENT=staging eas build --profile staging --platform android",
    "build:dev:android": "eas build --profile dev --platform android",
    "update:staging": "_UPDATE_ENVIRONMENT=staging eas update --branch staging",
    "update:prod": "_UPDATE_ENVIRONMENT=prod eas update --branch prod",
    "update:staging:ci": "EAS_NO_VCS=1 _UPDATE_ENVIRONMENT=staging eas update --branch $_ENVIRONMENT --non-interactive --message $COMMIT_SHA",
    "update:prod:ci": "EAS_NO_VCS=1 _UPDATE_ENVIRONMENT=prod eas update --branch $_ENVIRONMENT --non-interactive --message $_RELEASE_ID",
    "doctor": "_DEV_ENVIRONMENT=staging expo doctor",
    "test": "jest",
    "postinstall": "node ./eas_build/preInstall.mjs"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@sentry/react-native/)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/app.config.js",
      "!**/index.js",
      "!**/metro.config.js"
    ],
    "coverageDirectory": "./tests/coverage",
    "setupFiles": [
      "./tests/setup.ts"
    ],
    "roots": [
      ".",
      "../common"
    ],
    "modulePaths": [
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "@config-plugins/react-native-branch": "^4.0.0",
    "@expo/config-plugins": "^5.0.2",
    "@expo/prebuild-config": "^5.0.5",
    "@googlemaps/google-maps-services-js": "3.1.16",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/netinfo": "9.3.5",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@sentry/react-native": "4.9.0",
    "assert": "^2.0.0",
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "expo": "^47.0.0",
    "expo-application": "~5.0.1",
    "expo-constants": "~14.0.2",
    "expo-dev-client": "~2.0.1",
    "expo-device": "~5.0.0",
    "expo-linear-gradient": "~12.0.1",
    "expo-linking": "~3.3.0",
    "expo-location": "~15.0.1",
    "expo-modules-autolinking": "~1.0.0",
    "expo-notifications": "~0.17.0",
    "expo-screen-orientation": "~5.0.1",
    "expo-secure-store": "~12.0.0",
    "expo-splash-screen": "~0.17.5",
    "expo-status-bar": "~1.4.2",
    "expo-task-manager": "~11.0.1",
    "expo-updates": "~0.15.6",
    "inversify": "^5.1.1",
    "inversify-react": "^1.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.2",
    "mobx-persist-store": "^1.0.3",
    "mobx-react": "^7.2.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "parse-full-name": "^1.2.6",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-branch": "^5.4.0",
    "react-native-calendar-picker": "^7.1.2",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-maps": "1.3.2",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-scroll-into-view": "^2.0.2",
    "react-native-svg": "13.4.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-web": "~0.18.7",
    "react-native-wheel-picker-expo": "^0.4.2",
    "react-router-dom": "^5.2.0",
    "react-string-replace": "^1.1.0",
    "realm": "11.3.1",
