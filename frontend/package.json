{
  "scripts": {
    "dev": "_DEV_ENVIRONMENT=dev expo start --dev-client",
    "dev:staging": "_DEV_ENVIRONMENT=staging expo start --dev-client",
    "dev:prod": "_DEV_ENVIRONMENT=prod expo start --dev-client",
    "build:prod": "eas build --profile prod",
    "build:staging": "eas build --profile staging",
    "build:prod:ios": "eas build --profile prod --platform ios",
    "build:staging:ios": "eas build --profile staging --platform ios",
    "build:dev:ios": "eas build --profile dev --platform ios",
    "build:prod:android": "eas build --profile prod --platform android",
    "build:staging:android": "eas build --profile staging --platform android",
    "build:dev:android": "eas build --profile dev --platform android",
    "update:staging": "_UPDATE_ENVIRONMENT=staging eas update --branch staging",
    "update:prod": "_UPDATE_ENVIRONMENT=prod eas update --branch prod",
    "update:staging:ci": "_UPDATE_ENVIRONMENT=staging eas update --branch $_ENVIRONMENT --non-interactive --message $COMMIT_SHA",
    "update:prod:ci": "_UPDATE_ENVIRONMENT=prod eas update --branch $_ENVIRONMENT --non-interactive --message $_RELEASE_ID",
    "doctor": "_DEV_ENVIRONMENT=staging expo doctor",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|@sentry/react-native/)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/app.config.js",
      "!**/index.js",
      "!**/metro.config.js"
    ],
    "coverageDirectory": "./tests/coverage",
    "setupFiles": [
      "./tests/setup.ts"
    ],
    "roots": [
      ".",
      "../common"
    ],
    "modulePaths": [
      "<rootDir>/node_modules/"
    ]
  },
  "dependencies": {
    "@config-plugins/react-native-branch": "^3.0.0",
    "@expo/config-plugins": "^4.1.0",
    "@expo/prebuild-config": "^4.0.0",
    "@googlemaps/google-maps-services-js": "3.1.16",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@sentry/react-native": "^3.1.1",
    "assert": "^2.0.0",
    "axios": "^0.21.1",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.1",
    "events": "^3.3.0",
    "expo": "^45.0.0",
    "expo-application": "~4.1.0",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~1.0.1",
    "expo-device": "~4.2.0",
    "expo-linear-gradient": "~11.3.0",
    "expo-linking": "~3.1.0",
    "expo-location": "~14.2.2",
    "expo-modules-autolinking": "~0.8.1 || ~0.9.0",
    "expo-notifications": "~0.15.4",
    "expo-secure-store": "~11.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-task-manager": "~10.2.1",
    "expo-updates": "~0.13.4",
    "inversify": "^5.1.1",
    "inversify-react": "^1.0.0",
    "jest": "^26.6.3",
    "jest-expo": "^46.0.1",
    "lodash": "^4.17.21",
    "mobx": "^6.3.2",
    "mobx-persist-store": "^1.0.3",
    "mobx-react": "^7.2.0",
    "module-alias": "^2.2.2",
    "moment": "^2.29.1",
    "parse-full-name": "^1.2.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-branch": "^5.4.0",
    "react-native-calendar-picker": "^7.1.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-get-random-values": "~1.8.0",
    "react-native-maps": "0.30.2",
    "react-native-paper": "^4.9.2",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-scroll-into-view": "^2.0.2",
    "react-native-svg": "12.3.0",
    "react-native-vector-icons": "^8.1.0",
    "react-native-web": "0.17.7",
    "react-native-wheel-picker-expo": "^0.4.2",
    "react-router-dom": "^5.2.0",
    "react-string-replace": "^1.1.0",
    "reflect-metadata": "^0.1.13",
    "sentry-expo": "^4.2.0",
    "socket.io-client": "^4.4.0",
    "socket.io-mock": "^1.3.2",
    "stream-browserify": "^3.0.0",
    "url": "^0.11.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.18.9",
    "@expo/ngrok": "^2.5.0",
    "@testing-library/jest-native": "^4.0.11",
    "@testing-library/react-native": "^11.0.0",
    "@types/google.maps": "^3.45.6",
    "@types/jest": "^28.1.8",
    "@types/lodash": "^4.14.178",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.67.6",
    "@types/react-router-dom": "^5.1.8",
    "@types/uuid": "^8.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "eas-cli": "^2.4.1",
    "expo-cli": "^5.4.3",
    "react-test-renderer": "17",
    "typescript": "~4.3.5"
  },
  "private": true,
  "name": "frontend",
  "version": "1.0.0",
  "react-native": {
    "stream": "stream-browserify"
  }
}
